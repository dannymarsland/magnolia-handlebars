<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module SYSTEM "module.dtd" >
<module>
    <name>magnolia-handlebars</name>
    <displayName>Magnolia Handlebars</displayName>
    <description>Handlebars integration for Magnolia</description>
    <versionHandler>com.dannymarsland.magnolia.handlebars.helper.setup.HandlebarsVersionHandler</versionHandler>
    <version>1.0.0</version>
    <dependencies>
        <dependency>
            <name>core</name>
            <version>5.2/*</version>
        </dependency>
        <dependency>
            <name>rendering</name>
            <version>5.2/*</version>
        </dependency>
    </dependencies>
    <repositories>
        <repository>
            <name>magnolia</name>
            <workspaces>
                <workspace>handlebars</workspace>
            </workspaces>
        </repository>
    </repositories>
</module>